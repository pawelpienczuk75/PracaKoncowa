
@{
    ViewData["Title"] = "ViewTableOfWeek";
}
<!DOCTYPE html>
<html lang="pl-PL">

<p></p>
<h2>ViewTableOfWeek</h2>
<p></p>
<p></p>
<table id="example" class="display" style="width: 80%">
    <thead>
        <tr>
            <th>Godzina</th>
            <th>Poniedziałek</th>
            <th>Wtorek</th>
            <th>Środa</th>
            <th>Czwartek</th>
            <th>Piątek</th>
            <th>Sobota</th>
            <th>Niedziela</th>
            @for (int j = 0; j < 10; j++)
            {
            <tr>
                @for (int i = 0; i < 8; i++)
                {
                    <td>@i</td>
                }
            </tr>
        }
            </tr>
        </thead>
    </table>

    <!-- jQuery -->
    <script src="/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="/js/VievTableOfWeek.js" type="text/javascript"></script>


